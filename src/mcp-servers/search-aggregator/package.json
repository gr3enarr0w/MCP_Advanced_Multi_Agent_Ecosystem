{
  "name": "search-aggregator-mcp",
  "version": "1.0.0",
  "description": "Multi-provider search aggregation MCP server with caching and result ranking",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:html": "jest --coverage --coverageReporters=html",
    "test:coverage:json": "jest --coverage --coverageReporters=json",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "search-aggregation",
    "web-search",
    "typescript"
  ],
  "author": "Roo AI Development",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.0",
    "date-fns": "^4.1.0",
    "eventemitter3": "^5.0.1",
    "lodash": "^4.17.21",
    "mammoth": "^1.6.0",
    "pdf-parse": "^1.1.1",
    "sql.js": "^1.10.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.0",
    "@types/sql.js": "^1.4.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "axios-mock-adapter": "^2.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
